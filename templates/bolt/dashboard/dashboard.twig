{% extends 'bolt/base.html.twig' %}

{% block container "vuecontent" %}

{% block title "Dashboard" %}

{% block aside %}
    <div class="vue">
        <DashboardNews></DashboardNews>
    </div>
{% endblock %}



{% block body %}

{% set contentTypes = [
    {
        slug: 'pages'
    },
    {
        slug: 'homepage'
    }
] %}
{% for contentType in contentTypes if true or contentType.showOnDashboard %}
    <DashboardContentList type="{{ contentType.slug }}" limit="5"></DashboardContentList>
{% endfor %}

{% endblock %}

